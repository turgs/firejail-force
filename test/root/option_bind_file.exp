#!/usr/bin/expect -f

set timeout 10
spawn $env(SHELL)
match_max 100000

send -- "firejail --bind=tmpfile,/etc/passwd\r"
expect {
	timeout {puts "TESTING ERROR 0\n";exit}
	"Child process initialized"
}
sleep 1

send -- "cat /etc/passwd;pwd\r"
expect {
	timeout {puts "TESTING ERROR 1\n";exit}
	"hello"
}
sleep 1

puts "\n"
